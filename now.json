{
    "version": 2,
    "name": "rabiel",
    "builds": [
        { "src": "package.json", "use": "@now/static-build" }
    ],
    "build": { "env": { "NODE_PATH": "@node_path" } },

    "routes": [
      {"src": "/.*", "dest": "/index.html"}
    ],
    "alias": ["rabiel.dev", "my-alias"],
    "public": true,
    "collectMetrics": true
}